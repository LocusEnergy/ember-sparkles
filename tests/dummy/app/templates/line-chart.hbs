{{#ember-sparkles as |chart|}}

  {{chart.line-chart
    data=timeseriesData
    x-scale=(time-scale
      domain
      (append 0 chart.width)
    )

    y-scale=(linear-scale
      range
      (append chart.height 0)
    )

    ticks=5
    tick-format='%Y-%m-%d'
  }}

{{/ember-sparkles}}

<button {{action (throttle (action "toggleData") 500 true)}}>Toggle Data</button>